<?xml version="1.0" encoding="UTF-8"?>
<struct>
	<!-- 본인서명발급사실 확인 요청 -->
	<field name="SCAF_ID"		length="12"		type="AN"	default=""/>	<!-- SCAF ID -->
	<field name="ISSUE_TYPE"	length="1"		type="AN"	default=""/>	<!-- 등록번호 구분(1:주민등록번호 , 2:외국인등록번호) -->
	<field name="SSN"			length="13"		type="AN"	default=""/>	<!-- 주민등록번호(또는 외국인등록번호 -->
	<field name="DC_NO"			length="16"		type="N"	default=""/>	<!-- 문서번호 -->
	<field name="ISSUE_ORG"		length="50"		type="AN"	default=""/>	<!-- 발급기관 -->
	<field name="ISSUE_DATE"	length="8"		type="AN"	default=""/>	<!-- 발급일자 -->
	<field name="FLR"			length="98"		type="AN"	default=""/>	<!-- 공란 -->
</struct>
