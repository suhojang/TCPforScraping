		<field name="DC3_CF_RDT"		length="8"		type="AN"	default=""		xpath18="ESTBSDATE"	filter="DATE"/>	<!-- 저당권설정일 -->
		<field name="DC3_CF_TMDT"		length="8"		type="AN"	default=""		xpath18="TRMNATDATE"	filter="DATE"/>	<!-- 저당권말소일 -->
